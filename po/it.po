msgid ""
"Convert the file content from GBK to UTF8\n"
"iconv -f gbk -t utf8 FILE_NAME\n"
msgstr ""
"Converte il contenuto del file da GBK a UTF8\n"
"iconv -f gbk -t utf8 FILE_NAME\n"

#: ../ailurus/common/tips.py:211
msgid ""
"Convert tags in '*.mp3' from GBK to UTF8\n"
"find . -name '*.mp3' -execdir mid3iconv -e GBK {} \\;\n"
msgstr ""
"Converte i tag in '*.mp3' da GBK a UTF8\n"
" find . -name '*.mp3' -execdir mid3iconv -e GBK {} \\;\n"

#: ../ailurus/common/tips.py:215
