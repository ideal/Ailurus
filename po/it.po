#: ../ailurus/common/tips.py:28
msgid ""
"motto of Ailurus\n"
"Give a man a fish and he can eat for a day; but if you teach him how to "
"fish, he'll eat for a lifetime."
msgstr ""
"motto di Ailurus\n"
"Dai ad un uomo un pesce e lo sfami per un giorno: ma se gli insegni a "
"pescare lo sfami per la vita."

#: ../ailurus/common/tips.py:31
msgid ""
"Display Linux distributor's ID\n"
"lsb_release -is\n"
"\n"
"Display Linux release number\n"
"lsb_release -rs\n"
"\n"
"Display Linux code name\n"
"lsb_release -cs"
msgstr ""
"Mostra  l'ID del distributore Linux\n"
"lsb_release -is\n"
"\n"
"Mostra il numero di release Linux\n"
"lsb_release -rs\n"
"\n"
"Mostra il nome del codice Linux\n"
"lsb_release -cs"

#: ../ailurus/common/tips.py:41
msgid ""
"Display machine hardware name\n"
"uname -m"
msgstr ""
"Mostra il nome hardware della macchina\n"
"uname -m"

#: ../ailurus/common/tips.py:44
msgid ""
"List all PCI devices, such as display card and ethernet card.\n"
"lspci\n"
msgstr ""
"Elenca tutti i dispositivi PCI, come scheda grafica e scheda ethernet.\n"
"lspci\n"

#: ../ailurus/common/tips.py:48
msgid ""
"Reclaim memory which stores pagecache, dentries and inodes\n"
"echo 3 > /proc/sys/vm/drop_caches\n"
msgstr ""
"Reclama la memoria che contiene pagecache, dentries e inodes.\n"
"echo 3 > /proc/sys/vm/drop_caches\n"

#: ../ailurus/common/tips.py:52
msgid ""
"Display a list of modules in the Linux Kernel\n"
"lsmod\n"
msgstr ""
"Mostra una lisat di moduli nel Kernel Linix\n"
"lsmod\n"

#: ../ailurus/common/tips.py:56
msgid ""
"List USB devices\n"
"lsusb -v\n"
msgstr ""
"Elenca i dispositivi USB\n"
"lsusb -v\n"

#: ../ailurus/common/tips.py:60
msgid ""
"Display the status of ethernet card\n"
"sudo ethtool eth0\n"
msgstr ""
"Mostra lo status della scheda ethernet\n"
"sudo ethtool eth0\n"

#: ../ailurus/common/tips.py:64
msgid ""
"List hardware\n"
"sudo lshw\n"
msgstr ""
"Elenca l'hardware\n"
"sudo lshw\n"

#: ../ailurus/common/tips.py:68
msgid ""
"List harddisk partitions\n"
"sudo fdisk -l\n"
msgstr ""
"Elenca le partizione del disco rigido\n"
"sudo fdisk -l\n"

#: ../ailurus/common/tips.py:72
msgid ""
"Display SATA harddisk parameters\n"
"sudo hdparm -I /dev/sda\n"
msgstr ""
"Mostra i parametri del disco rigido SATA\n"
"sudo hdparm -I /dev/sda\n"

#: ../ailurus/common/tips.py:76
msgid ""
"Display disk space usage\n"
"df -h\n"
msgstr ""
"Mostra l'uso dello spazio del disco\n"
"df -h\n"

#: ../ailurus/common/tips.py:80
msgid ""
"Display file/folder space usage\n"
"du -bsh FOLDER_NAME\n"
msgstr ""
"Mostra lo spazio utilizzato dal file/folder\n"
"du -bsh FOLDER_NAME\n"

#: ../ailurus/common/tips.py:84
msgid ""
"Display amount of free and used memory\n"
"free\n"
msgstr ""
"Mostra l'ammontare di memoria libera e usata\n"
"free\n"

#: ../ailurus/common/tips.py:88
msgid ""
"Display processes\n"
"ps -e\n"
"Display a tree of processes\n"
"pstree\n"
"Display processes dynamically\n"
"top\n"
msgstr ""
"Mostra i processi\n"
"ps -e\n"
"Mostra un albero di processi\n"
"pstree\n"
"Mostra i processi dinamicamente\n"
"top\n"

#: ../ailurus/common/tips.py:96
msgid ""
"Terminate a process with a given process id\n"
"sudo kill -9 PROCESS_ID\n"
msgstr ""
"Termina un processo con un dato id\n"
"sudo kill -9 PROCESS_ID\n"

#: ../ailurus/common/tips.py:100
msgid ""
"Terminate all processes with a given name\n"
"sudo killall PROCESS_NAME\n"
msgstr ""
"Termina tutti i processi con un determinato nome\n"
"sudo killall PROCESS_NAME\n"

#: ../ailurus/common/tips.py:104
msgid ""
"List files which are opened by a given process\n"
"lsof -p PROCESS_ID\n"
"lsof -c PROCESS_NAME\n"
msgstr ""
"Elenca i files che sono aperti da un determinato processo\n"
"lsof -p PROCESS_ID\n"
"lsof -c PROCESS_NAME\n"

#: ../ailurus/common/tips.py:109
msgid ""
"List processes which opened a given file\n"
"lsof FILE_NAME\n"
msgstr ""
"Elenca i processi che aprono un determinato file\n"
"lsof FILE_NAME\n"

#: ../ailurus/common/tips.py:113
msgid ""
"List processes which are using port 80\n"
"lsof -i :80\n"
msgstr ""
"Elenca i processi che stanno usando la porta 80\n"
"lsof -i :80\n"

#: ../ailurus/common/tips.py:117
msgid ""
"Configure an ADSL connection\n"
"sudo pppoeconf\n"
msgstr ""
"Configura una connessione ADSL\n"
"sudo pppoeconf\n"

#: ../ailurus/common/tips.py:121
msgid ""
"Starts up ADSL connections\n"
"sudo pon\n"
"Shuts down ADSL connections\n"
"sudo poff\n"
msgstr ""
"Fa partire una connessione ADSL\n"
"sudo pon\n"
"Chiude una connessione ADSL\n"
"sudo poff\n"

#: ../ailurus/common/tips.py:127
msgid ""
"Display MAC of a given IP address\n"
"arping IP_ADDRESS\n"
msgstr ""
"Mostra il MAC di un dato indirizzo IP\n"
"arping IP_ADDRESS\n"

#: ../ailurus/common/tips.py:131
msgid ""
"Display NetBIOS name of a given IP address\n"
"nmblookup -A IP_ADDRESS\n"
msgstr ""
"Mostra il nome NetBIOS di un dato indirizzo IP\n"
"nmblookup -A IP_ADDRESS\n"

#: ../ailurus/common/tips.py:135
msgid ""
"Display IP address and MAC\n"
"ifconfig -a\n"
msgstr ""
"Mostra indirizzo IP e MAC\n"
"ifconfig -a\n"

#: ../ailurus/common/tips.py:139
msgid ""
"Display route\n"
"netstat -rn\n"
msgstr ""
"Mostra percorsi di rete\n"
"netstat -rn\n"

#: ../ailurus/common/tips.py:143
msgid ""
"Set MAC of ethernet interface\n"
"sudo ifconfig eth0 hw ether 00:11:22:33:44:55\n"
msgstr ""
"Definisce il MAC dell'interfaccia ethernet\n"
"sudo ifconfig eth0 hw ether 00:11:22:33:44:55\n"

#: ../ailurus/common/tips.py:147
msgid ""
"Display information of a domain name\n"
"whois example.com\n"
msgstr ""
"Mostra informazioni di un domain name\n"
"whois example.com\n"

#: ../ailurus/common/tips.py:151
msgid ""
"Display the network path to a given host\n"
"tracepath example.com\n"
msgstr ""
"Mostra la network path per un host specifico\n"
"tracepath example.com\n"

#: ../ailurus/common/tips.py:155
msgid ""
"Request an IP address from DHCP server\n"
"sudo dhclient\n"
msgstr ""
"Richiede un indirizzo IP da un server DHCP\n"
"sudo dhclient\n"

#: ../ailurus/common/tips.py:159
msgid ""
"Temporarily restart an init script\n"
"sudo /etc/init.d/SCRIPT_NAME restart\n"
"Temporarily stop an init script\n"
"sudo /etc/init.d/SCRIPT_NAME stop\n"
msgstr ""
"Riavvia temporaneamente un init script\n"
"sudo /etc/init.d/SCRIPT_NAME restart\n"
"Ferma temporaneamente un init script\n"
"sudo /etc/init.d/SCRIPT_NAME stop\n"

#: ../ailurus/common/tips.py:165
msgid ""
"Add a user\n"
"sudo adduser USER_NANE\n"
"Delete a user\n"
"sudo deluser USER_NAME\n"
msgstr ""
"Aggiungi un utente\n"
"sudo adduser USER_NAME\n"
"Cancella un utente\n"
"sudo deluser USER_NAME\n"

#: ../ailurus/common/tips.py:171
msgid ""
"Change user password\n"
"sudo passwd USER_NAME\n"
msgstr ""
"Cambia la password dell'utente\n"
"sudo passwd USER_NAME\n"

#: ../ailurus/common/tips.py:175
msgid ""
"Changes user fullname, office number, office extension, and home phone "
"number information.\n"
"sudo chfn USER_NAME\n"
"Display user information\n"
"finger USER_NAME\n"
msgstr ""
"Cambia il nome completo dell'utente, numero d'ufficio,  estensione "
"dell'ufficio, telefono di casa e informazioni\n"
"sudo chfn USER_NAME\n"
"Mostra le informazioni dell'utente\n"
"finger USER_NAME\n"

#: ../ailurus/common/tips.py:181
msgid ""
"Temporarily prevent a user from logging in\n"
"sudo usermod -L USER_NAME\n"
"Revoke the operation above\n"
"sudo usermod -U USER_NAME\n"
msgstr ""
"Previene temporaneamente un utente dal log in\n"
"sudo usermod -L USER_NAME\n"
"Annulla l'operazione sopra\n"
"sudo usermod -U USER_NAME\n"

#: ../ailurus/common/tips.py:187
msgid ""
"Add a user to admin group\n"
"sudo usermod -G admin -a USER_NAME\n"
msgstr ""
"Aggiunge un utente al gruppo amministatori\n"
"sudo usermod -G admin -a USER_NAME\n"

#: ../ailurus/common/tips.py:191
msgid ""
"Set the HTTP proxy\n"
"export http_proxy=http://PROXY.DOMAIN.NAME:PORT\n"
msgstr ""
"Imposta il proxy HTTP \n"
"export http_proxy=http://PROXY.DOMAIN.NAME:PORT\n"

#: ../ailurus/common/tips.py:195
msgid ""
"Modify the information displayed after logging in\n"
"sudo vim /etc/motd.tail\n"
msgstr ""
"Modifica le informazioni mostrate dopo il log in\n"
"sudo vim /etc/motd.tail\n"

#: ../ailurus/common/tips.py:199
msgid ""
"Choose the input method for X Window\n"
"im-switch -c\n"
msgstr ""
"Mostra il metodo di input per X Window\n"
"im-switch -c\n"

#: ../ailurus/common/tips.py:203
msgid ""
"Convert the file name from GBK to UTF8\n"
"convmv -r -f gbk -t utf8 --notest FILE_NAME\n"
msgstr ""
"Converte il nome del file da GBK a UTF8\n"
"convmv -r -f gbk -t utf8 --notest FILE_NAME\n"

