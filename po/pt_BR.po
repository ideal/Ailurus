msgid ""
"List files which are opened by a given process\n"
"lsof -p PROCESS_ID\n"
"lsof -c PROCESS_NAME\n"
msgstr ""
"Lista os arquivos que são abertos por um determinado processo\n"
"lsof -p ID_DO_PROCESSO\n"
"lsof -c NOME_DO_PROCESSO\n"

#: ../ailurus/common/tips.py:109
msgid ""
"List processes which opened a given file\n"
"lsof FILE_NAME\n"
msgstr ""
"Lista os processos que abriram um determinado arquivo\n"
"lsof NOME_DO_ARQUIVO\n"

